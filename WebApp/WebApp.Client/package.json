{
    "name": "portfolio-backtest",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "author": {
        "name": "Chris O'Brien",
        "url": "https://github.com/prettycode"
    },
    "license": "cc-by-nc-4.0",
    "scripts": {
        "clean:obj": "rmdir obj /s /q || exit /b 0",
        "clean:dist": "rmdir dist /s /q || exit /b 0",
        "clean:node_modules": "rmdir node_modules /s /q || exit /b 0",
        "postclean:node_modules": "npm run install:fix",
        "clean:all": "npm run clean:obj && npm run clean:dist && npm run clean:node_modules && npm run clean:node_modules",
        "start": "npm run install:ci && npm run start:vite",
        "preview": "vite preview",
        "prestart:withserver": "npm run clean:node_modules",
        "start:withserver": "cd ..\\..\\ && start pwsh .\\dotnet-pipeline.ps1 \"clean, build, test, run:WebApp.Server\"",
        "start:vite": "npx vite --open",
        "versions": "echo node: && node -v && echo npm: && npm -v",
        "install:ci": "npm ci",
        "install:fix": "npm install",
        "outdated:ci": "npm outdated || exit /b 0",
        "outdated:fix": "npm update --save",
        "audit:ci": "npm audit --audit-level=none",
        "audit:fix": "npm audit fix --force",
        "prettier:ci": "npx prettier . --check",
        "prettier:fix": "npx prettier . --write",
        "lint:ci": "npx eslint . --max-warnings=0",
        "lint:fix": "npx eslint . --fix",
        "vite:build:dev": "npx tsc & npx vite build --mode development",
        "vite:build:ci": "npx tsc && npx vite build",
        "build:dev": "npm run versions && npm run install:fix && npm run outdated:fix && npm run audit:fix && npm run prettier:fix && npm run lint:fix && npm run vite:build:dev && npm run test",
        "build:ci": "npm run versions && npm run install:ci && npm run outdated:ci & npm run audit:ci && npm run prettier:ci && npm run lint:ci && npm run vite:build:ci && npm run test",
        "test:dev": "npm run vite:build:dev && npx jest",
        "test": "npx jest",
        "precommit": "npm run build:dev && npm run build:ci && npm run start:vite"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "bootstrap": "^5.3.3",
        "decimal.js": "^10.4.3",
        "font-awesome": "^4.7.0",
        "highcharts": "^11.4.8",
        "highcharts-react-official": "^3.2.1",
        "lodash.clonedeep": "^4.5.0",
        "lodash.groupby": "^4.6.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-select": "^5.8.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@types/lodash.clonedeep": "^4.5.9",
        "@types/lodash.groupby": "^4.6.9",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/react-fontawesome": "^1.6.8",
        "@vitejs/plugin-react": "4.3.1",
        "eslint": "^9.10.0",
        "eslint-plugin-react": "^7.35.2",
        "globals": "^15.9.0",
        "prettier": "3.3.3",
        "typescript-eslint": "^8.5.0",
        "vite": "5.4.3"
    }
}
